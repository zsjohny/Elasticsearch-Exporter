  "bugs": {
    "url": "http://github.com/mallocator/Elasticsearch-Exporter/issues",
    "email": "mallox@pyxzl.net"
  },
  "license": "Apache-2.0",
  "version": "2.0.0",
  "os": [
    "darwin",
    "linux"
  ],
  "author": "Ravi Gairola <mallox@pyxzl.net>",
